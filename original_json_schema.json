{
    "$defs": {
        "GenbankSequence": {
            "description": "A class to store sequences and features in genbank model\n    ",
            "properties": {
                "type": {
                    "default": "file",
                    "title": "Type",
                    "type": "string"
                },
                "file_extension": {
                    "default": "gb",
                    "title": "File Extension",
                    "type": "string"
                },
                "file_content": {
                    "default": "",
                    "title": "File Content",
                    "type": "string"
                },
                "overhang_crick_3prime": {
                    "default": 0,
                    "description": "Taken from pydna's `dseq::ovhg`        An integer describing the length of the        crick strand overhang in the 5' of the molecule, or 3' of the crick strand",
                    "title": "Overhang Crick 3Prime",
                    "type": "integer"
                },
                "overhang_watson_3prime": {
                    "default": 0,
                    "description": "The equivalent of `overhang_crick_3prime`        but for the watson strand",
                    "title": "Overhang Watson 3Prime",
                    "type": "integer"
                }
            },
            "title": "GenbankSequence",
            "type": "object"
        },
        "PrimerModel": {
            "description": "Called PrimerModel not to be confused with the class from pydna.",
            "properties": {
                "id": {
                    "title": "Id",
                    "type": "integer"
                },
                "name": {
                    "title": "Name",
                    "type": "string"
                },
                "sequence": {
                    "pattern": "^[acgtACGT]+$",
                    "title": "Sequence",
                    "type": "string"
                }
            },
            "required": [
                "id",
                "name",
                "sequence"
            ],
            "title": "PrimerModel",
            "type": "object"
        },
        "SequenceEntity": {
            "properties": {
                "id": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Id"
                },
                "kind": {
                    "default": "entity",
                    "title": "Kind",
                    "type": "string"
                },
                "sequence": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/GenbankSequence"
                        },
                        {
                            "type": "null"
                        }
                    ]
                }
            },
            "required": [
                "sequence"
            ],
            "title": "SequenceEntity",
            "type": "object"
        },
        "Source": {
            "description": "A class to represent sources of DNA\n    ",
            "properties": {
                "id": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Id"
                },
                "kind": {
                    "default": "source",
                    "title": "Kind",
                    "type": "string"
                },
                "input": {
                    "default": [],
                    "items": {
                        "type": "integer"
                    },
                    "title": "Input",
                    "type": "array"
                },
                "output": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Output"
                },
                "type": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/SourceType"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "info": {
                    "default": {},
                    "title": "Info",
                    "type": "object"
                }
            },
            "required": [
                "type"
            ],
            "title": "Source",
            "type": "object"
        },
        "SourceType": {
            "enum": [
                "repository_id",
                "file",
                "restriction",
                "sticky_ligation",
                "PCR",
                "homologous_recombination"
            ],
            "title": "SourceType",
            "type": "string"
        }
    },
    "description": "A class to represent a cloning strategy\n    ",
    "properties": {
        "sources": {
            "items": {
                "$ref": "#/$defs/Source"
            },
            "title": "Sources",
            "type": "array"
        },
        "sequences": {
            "items": {
                "$ref": "#/$defs/SequenceEntity"
            },
            "title": "Sequences",
            "type": "array"
        },
        "primers": {
            "items": {
                "$ref": "#/$defs/PrimerModel"
            },
            "title": "Primers",
            "type": "array"
        }
    },
    "required": [
        "sources",
        "sequences",
        "primers"
    ],
    "title": "CloningStrategy",
    "type": "object"
}