{
    "$defs": {
        "GenomeCoordinatesSource": {
            "additionalProperties": false,
            "description": "Represents the source of a sequence that is identified by genome coordinates, requested from NCBI",
            "properties": {
                "assembly_accession": {
                    "description": "The accession of the assembly",
                    "type": "string"
                },
                "gene_id": {
                    "description": "The gene id of the sequence",
                    "type": "integer"
                },
                "id": {
                    "description": "A unique identifier for a thing",
                    "type": "integer"
                },
                "input": {
                    "description": "Identifiers of the sequences that are an input to this source. If the source represents external import of a sequence, it's empty.",
                    "items": {
                        "type": "integer"
                    },
                    "type": "array"
                },
                "kind": {
                    "description": "The kind entity (always equal to \"source\"). Should probably be removed.",
                    "type": "string"
                },
                "locus_tag": {
                    "description": "The locus tag of the sequence",
                    "type": "string"
                },
                "output": {
                    "description": "Identifier of the sequence that is the output of this source.",
                    "type": "integer"
                },
                "sequence_accession": {
                    "description": "The accession of the sequence",
                    "type": "string"
                },
                "start": {
                    "description": "The starting coordinate (1-based) of the sequence in the sequence accession",
                    "type": "integer"
                },
                "stop": {
                    "description": "The ending coordinate (1-based) of the sequence in the sequence accession",
                    "type": "integer"
                },
                "strand": {
                    "description": "The strand of the sequence in the sequence accession, should be 1 or -1",
                    "type": "integer"
                },
                "type": {
                    "$ref": "#/$defs/SourceType",
                    "description": "The type of the source"
                }
            },
            "required": [
                "sequence_accession",
                "start",
                "stop",
                "strand",
                "type",
                "kind",
                "id"
            ],
            "then": {
                "properties": {
                    "type": {
                        "anyOf": [
                            {
                                "const": "repository_id"
                            }
                        ]
                    }
                },
                "required": [
                    "type"
                ]
            },
            "title": "GenomeCoordinatesSource",
            "type": "object"
        },
        "ManuallyTypedSource": {
            "additionalProperties": false,
            "description": "Represents the source of a sequence that is manually typed by the user",
            "properties": {
                "id": {
                    "description": "A unique identifier for a thing",
                    "type": "integer"
                },
                "input": {
                    "description": "Identifiers of the sequences that are an input to this source. If the source represents external import of a sequence, it's empty.",
                    "items": {
                        "type": "integer"
                    },
                    "type": "array"
                },
                "kind": {
                    "description": "The kind entity (always equal to \"source\"). Should probably be removed.",
                    "type": "string"
                },
                "output": {
                    "description": "Identifier of the sequence that is the output of this source.",
                    "type": "integer"
                },
                "type": {
                    "$ref": "#/$defs/SourceType",
                    "description": "The type of the source"
                },
                "user_input": {
                    "pattern": "^[acgtACGT]+$",
                    "type": "string"
                }
            },
            "required": [
                "type",
                "kind",
                "id"
            ],
            "then": {
                "properties": {
                    "type": {
                        "anyOf": [
                            {
                                "const": "manually_typed"
                            }
                        ]
                    }
                },
                "required": [
                    "type"
                ]
            },
            "title": "ManuallyTypedSource",
            "type": "object"
        },
        "NamedThing": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "id": {
                    "description": "A unique identifier for a thing",
                    "type": "integer"
                }
            },
            "required": [
                "id"
            ],
            "title": "NamedThing",
            "type": "object"
        },
        "RepositoryIdSource": {
            "additionalProperties": false,
            "description": "Represents the source of a sequence that is identified by a repository id",
            "properties": {
                "id": {
                    "description": "A unique identifier for a thing",
                    "type": "integer"
                },
                "input": {
                    "description": "Identifiers of the sequences that are an input to this source. If the source represents external import of a sequence, it's empty.",
                    "items": {
                        "type": "integer"
                    },
                    "type": "array"
                },
                "kind": {
                    "description": "The kind entity (always equal to \"source\"). Should probably be removed.",
                    "type": "string"
                },
                "output": {
                    "description": "Identifier of the sequence that is the output of this source.",
                    "type": "integer"
                },
                "repository_id": {
                    "description": "The id of the sequence in the repository",
                    "type": "string"
                },
                "repository_name": {
                    "$ref": "#/$defs/RepositoryName"
                },
                "type": {
                    "$ref": "#/$defs/SourceType",
                    "description": "The type of the source"
                }
            },
            "required": [
                "repository_name",
                "repository_id",
                "type",
                "kind",
                "id"
            ],
            "then": {
                "properties": {
                    "type": {
                        "anyOf": [
                            {
                                "const": "repository_id"
                            }
                        ]
                    }
                },
                "required": [
                    "type"
                ]
            },
            "title": "RepositoryIdSource",
            "type": "object"
        },
        "RepositoryName": {
            "description": "",
            "enum": [
                "addgene",
                "genbank"
            ],
            "title": "RepositoryName",
            "type": "string"
        },
        "SequenceFileFormat": {
            "description": "",
            "enum": [
                "fasta",
                "genbank",
                "snapgene"
            ],
            "title": "SequenceFileFormat",
            "type": "string"
        },
        "SourceType": {
            "description": "",
            "enum": [
                "repository_id",
                "file",
                "restriction",
                "ligation",
                "PCR",
                "homologous_recombination",
                "gibson_assembly",
                "restriction_and_ligation",
                "genome_coordinates",
                "manually_typed"
            ],
            "title": "SourceType",
            "type": "string"
        },
        "UploadedFileSource": {
            "additionalProperties": false,
            "description": "Represents the source of a sequence that is uploaded as a file",
            "properties": {
                "file_format": {
                    "$ref": "#/$defs/SequenceFileFormat",
                    "description": "The format of the file"
                },
                "file_name": {
                    "description": "The name of the file",
                    "type": "string"
                },
                "id": {
                    "description": "A unique identifier for a thing",
                    "type": "integer"
                },
                "index_in_file": {
                    "description": "The index of the sequence in the file",
                    "type": "integer"
                },
                "input": {
                    "description": "Identifiers of the sequences that are an input to this source. If the source represents external import of a sequence, it's empty.",
                    "items": {
                        "type": "integer"
                    },
                    "type": "array"
                },
                "kind": {
                    "description": "The kind entity (always equal to \"source\"). Should probably be removed.",
                    "type": "string"
                },
                "output": {
                    "description": "Identifier of the sequence that is the output of this source.",
                    "type": "integer"
                },
                "type": {
                    "$ref": "#/$defs/SourceType",
                    "description": "The type of the source"
                }
            },
            "required": [
                "type",
                "kind",
                "id"
            ],
            "then": {
                "properties": {
                    "type": {
                        "anyOf": [
                            {
                                "const": "file"
                            }
                        ]
                    }
                },
                "required": [
                    "type"
                ]
            },
            "title": "UploadedFileSource",
            "type": "object"
        }
    },
    "$id": "https://w3id.org/genestorian/ShareYourCloning_LinkML",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "additionalProperties": true,
    "metamodel_version": "1.7.0",
    "title": "ShareYourCloning_LinkML",
    "type": "object",
    "version": null
}