    skos:inScheme <https://w3id.org/genestorian/ShareYourCloning_LinkML> .

shareyourcloning_linkml:strand a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "strand" ;
    skos:definition "The strand of the sequence in the sequence accession, should be 1 or -1" ;
    skos:inScheme <https://w3id.org/genestorian/ShareYourCloning_LinkML> .

shareyourcloning_linkml:user_input a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "user_input" ;
    skos:inScheme <https://w3id.org/genestorian/ShareYourCloning_LinkML> .

shareyourcloning_linkml:Source a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Source" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom shareyourcloning_linkml:SourceType ;
            owl:onProperty shareyourcloning_linkml:type ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty shareyourcloning_linkml:type ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty shareyourcloning_linkml:output ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty shareyourcloning_linkml:kind ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty shareyourcloning_linkml:kind ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty shareyourcloning_linkml:output ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty shareyourcloning_linkml:info ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty shareyourcloning_linkml:input ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty shareyourcloning_linkml:info ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty shareyourcloning_linkml:input ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty shareyourcloning_linkml:output ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty shareyourcloning_linkml:type ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty shareyourcloning_linkml:info ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty shareyourcloning_linkml:kind ],
        shareyourcloning_linkml:NamedThing ;
    skos:definition "Represents the source of a sequence" ;
    skos:inScheme <https://w3id.org/genestorian/ShareYourCloning_LinkML> .

shareyourcloning_linkml:RepositoryName a owl:Class,
        linkml:EnumDefinition ;
    owl:unionOf ( <https://w3id.org/genestorian/ShareYourCloning_LinkML/RepositoryName#addgene> <https://w3id.org/genestorian/ShareYourCloning_LinkML/RepositoryName#genbank> ) ;
    linkml:permissible_values <https://w3id.org/genestorian/ShareYourCloning_LinkML/RepositoryName#addgene>,
        <https://w3id.org/genestorian/ShareYourCloning_LinkML/RepositoryName#genbank> .

shareyourcloning_linkml:type a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "type" ;
    rdfs:range shareyourcloning_linkml:SourceType ;
    skos:definition "The type of the source" ;
    skos:inScheme <https://w3id.org/genestorian/ShareYourCloning_LinkML> .

shareyourcloning_linkml:SourceType a owl:Class,
        linkml:EnumDefinition ;
    owl:unionOf ( <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#repository_id> <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#file> <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#restriction> <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#ligation> <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#PCR> <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#homologous_recombination> <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#gibson_assembly> <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#restriction_and_ligation> <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#genome_coordinates> <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#manually_typed> ) ;
    linkml:permissible_values <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#PCR>,
        <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#file>,
        <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#genome_coordinates>,
        <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#gibson_assembly>,
        <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#homologous_recombination>,
        <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#ligation>,
        <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#manually_typed>,
        <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#repository_id>,
        <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#restriction>,
        <https://w3id.org/genestorian/ShareYourCloning_LinkML/SourceType#restriction_and_ligation> .

<https://w3id.org/genestorian/ShareYourCloning_LinkML> a owl:Ontology ;
    rdfs:label "ShareYourCloning_LinkML" ;
    dcterms:license "MIT" ;
    dcterms:title "ShareYourCloning_LinkML" ;
