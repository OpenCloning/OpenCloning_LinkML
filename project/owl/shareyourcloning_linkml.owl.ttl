    rdfs:subClassOf shareyourcloning_linkml:SourceType .

shareyourcloning_linkml:input a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "input" ;
    rdfs:range linkml:Integer ;
    skos:definition "Identifiers of the sequences that are an input to this source. If the source represents external import of a sequence, it's empty." ;
    skos:inScheme <https://w3id.org/genestorian/ShareYourCloning_LinkML> .

shareyourcloning_linkml:assembly_accession a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "assembly_accession" ;
    skos:definition "The accession of the assembly" ;
    skos:inScheme <https://w3id.org/genestorian/ShareYourCloning_LinkML> .

shareyourcloning_linkml:file_name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "file_name" ;
    skos:definition "The name of the file" ;
    skos:inScheme <https://w3id.org/genestorian/ShareYourCloning_LinkML> .

shareyourcloning_linkml:gene_id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "gene_id" ;
    skos:definition "The gene id of the sequence" ;
    skos:inScheme <https://w3id.org/genestorian/ShareYourCloning_LinkML> .

shareyourcloning_linkml:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Integer ;
